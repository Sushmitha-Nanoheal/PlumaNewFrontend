{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest"
  },
  "plugins": ["react"],
  "env": {
    "browser": true
  },
  "rules": {
    "react/prop-types": 0
  },
  "globals": {
    "process": true
  }
}
